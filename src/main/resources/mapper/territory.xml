<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhongwang.data.dao.TerritoryMapper" >
    <resultMap id="BaseResultMap" type="com.zhongwang.data.entity.Territory" >
        <result column="pro_name" property="companyId" jdbcType="VARCHAR" />
        <result column="pro_id" property="rank" jdbcType="VARCHAR" />
    </resultMap>
    <!--地域查询-->
    <select id="selectTerritory" resultType="com.zhongwang.data.entity.Territory">
        select pro_name as proName from territory
    </select>

</mapper>
